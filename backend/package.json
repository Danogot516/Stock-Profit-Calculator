{
  "name": "stock-calculator-backend",
  "version": "1.0.0",
  "description": "backend service for stock profit calculator",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "env-cmd -f ./config/.env.dev nodemon src/index.js",
    "seed-database": "env-cmd -f ./config/.env.db node src/scripts/seedDatabase.js",
    "seed-database-minutes": "env-cmd -f ./config/.env.db node src/scripts/seedDatabaseMinutes.js",
    "clear-cache": "node src/scripts/clearCache.js",
    "test": "env-cmd -f ./config/.env.test jest --watch --runInBand"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "minimist": "^1.2.8",
    "mongodb": "^5.3.0",
    "mongoose": "^7.0.5",
    "persistent-cache": "^1.1.2",
    "piscina": "^3.2.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.16",
    "supertest": "^6.3.3"
  }
}
